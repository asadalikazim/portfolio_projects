<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_nav_registration"
    app:startDestination="@id/regFrag">

    <fragment
        android:id="@+id/regFrag"
        android:name="com.kgc.su.features.registration.reg.RegFrag"
        android:label="frag_ra_reg"
        tools:layout="@layout/frag_ra_reg" >
        <action
            android:id="@+id/action_regFrag_to_signInFrag"
            app:destination="@id/signInFrag" />
        <action
            android:id="@+id/action_regFrag_to_signUpFrag"
            app:destination="@id/signUpFrag" />
    </fragment>
    <fragment
        android:id="@+id/signInFrag"
        android:name="com.kgc.su.features.registration.signIn.SignInFrag"
        android:label="frag_ra_sign_in"
        tools:layout="@layout/frag_ra_sign_in" />
    <fragment
        android:id="@+id/signUpFrag"
        android:name="com.kgc.su.features.registration.signUp.SignUpFrag"
        android:label="frag_ra_sign_up"
        tools:layout="@layout/frag_ra_sign_up" />
</navigation>